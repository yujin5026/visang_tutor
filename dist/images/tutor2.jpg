<script>
var _SOCIAL_URI_AUTH_ = '/ajax/social/auth';
var _SOCIAL_URI_API_ = '/ajax/social/api';
var _SOCIAL_URI_UI_ = '/ajax/social/ui';

$(document).ready(function(){
    // 이벤트 등록
    eventInit();
    // 댓글 리스트 가져오기 
    getReplyList($("#sr_news_id").val());
});

/* 이벤트 등록 */
function eventInit() {
    
    // 페이스북 로그인 버튼 클릭시
    $("#sr_login_btn_FB").unbind('click').bind('click',function(){ 
        doLogin("FB");
    });

    // 로그아웃 버튼 클릭시
    $("#sr_logout_btn").unbind('click').bind('click',function(){
        var authKey = $('#sr_user_key').val();
        if(authKey) {
            if(confirm('로그아웃 하시겠습니까?')) { 
                doLogout();
            }
        }
    });

    // 등록 버튼 클릭시
    $("#btn_reply_write").unbind('click').bind('click',function(){
        console.log("btn_reply_write");
        //setBtnDisable();
        var socialType = $("#sr_rep_social_type").val();
        var socialKey = $("#sr_rep_social_key").val();
        
        var sr_user_key = $("#sr_user_key").val();
        var snsCont = $("#textarea_reply").val();

        var newsId = $("#sr_news_id").val();
        var newsContens = $("#sr_news_contents").val();
        var newsTitle = $("#sr_news_title").val();
        var newsImage = $("#sr_news_image").val();

        var snsContLen = $("#textarea_reply").val().length;

        if(snsContLen<5){
            alert('한글 5자 이상 입력하셔야 합니다.');
            setBtnEnable();
            return false;
        }
            
        if(snsContLen>300){
            alert('최대 300자를 초과할 수 없습니다.');
            setBtnEnable();
            return false;
        }

        setReplyWrite(sr_user_key, socialType, socialKey, newsId, snsCont, newsContens, newsTitle, newsImage);

    });

    // 댓글 입력시
    $("#textarea_reply").keyup(function() {
        var textarealen = $(this).val().length;
        $(this).parent("div").find("#textarea_len").find("strong").html(textarealen);
    });

    // 댓글 더보기 클릭시
    $("#reply_list_add").unbind('click').bind('click',function(){   
        var newsId = $("#sr_news_id").val();
        //$(this).remove();
        getReplyListAdd(newsId);
    });

    // 댓글 코멘트 입력시
    $(".txtarea_reply").keyup(function(){   
        var txtarealen = $(this).val().length;
        $(this).parent("div").find("#txtarea_len").find("strong").html(txtarealen);
    });

    // 댓글 삭제
    $(".delete_reply").unbind('click').bind('click',function(){ 
        if(!confirm('정말 삭제 하시겠습니까?')) {
            return false;
        }
        var newsId = $("#sr_news_id").val();
        var snsKey = $(this).attr('snsKey');
        var authKey = $(this).attr('authKey');
        deleteReply(snsKey, newsId, authKey);
    });

    // 댓글 코멘트 등록 
    $(".btn_comment_write").unbind('click').bind('click',function(){    
        var snsKey = $(this).attr('snsKey');
        var newsId = $("#sr_news_id").val();
        var socialKey = $("#sr_rep_social_key").val();  
        var socialType = $("#sr_rep_social_type").val();
        var snsCmt = $(this).parent("div").children("textarea").val();

        var snsCmtLen = snsCmt.length;

        if(snsCmtLen < 5){
            alert('한글 5자 이상 입력하셔야 합니다.');
            return false;
        }
            
        if(snsCmtLen > 300){
            alert('최대 300자를 초과할 수 없습니다.');
            return false;
        }
        
        setCommentWrite(snsKey, newsId, socialKey, socialType, snsCmt);
    });

    // 댓글 코멘트 삭제
    $(".delete_comment").unbind('click').bind('click',function(){   
        if(!confirm('정말 삭제 하시겠습니까?')) {
            return false;
        }
        var snsKey = $(this).attr('snsKey');
        var cmtKey = $(this).attr('cmtKey');
        var authKey = $(this).attr('authKey');
        deleteComment(cmtKey, snsKey, authKey);
    });

    // 댓글 추천, 비추천 클릭시
    $(".reply_action").unbind('click').bind('click',function(){ 
        var action = $(this).attr('type');
        var snsKey = $(this).parent('ul').attr('snskey');
        var newsId = $("#sr_news_id").val();
        setReplyAction(snsKey, action, newsId);
    });

    // 댓글 코멘트 추천, 비추천 클릭시
    $(".cmt_action").unbind('click').bind('click',function(){   
        var action = $(this).attr('type');
        var snsKey = $(this).parent('ul').attr('snskey');
        var cmtKey = $(this).parent('ul').attr('cmtKey');
        setCmtAction(snsKey, cmtKey, action);
    });

}

/* 로그인 */
function doLogin(type) {
    $.ajax({
        url: _SOCIAL_URI_AUTH_,
        data: {'action':'login', 'type':type, 'rdUrl':$('#sr_rd_url').val()},
        success: function(data) {
            var json = jQuery.parseJSON(data);
            var loginUrl = json.loginUrl;
            $(location).attr('href', loginUrl);
        } 
    });
}

/* 로그아웃 */
function doLogout() {
    $.ajax({
        url: _SOCIAL_URI_AUTH_,
        data: {'action':'logout'},
        success: function(data) {
            var json = jQuery.parseJSON(data);
            if(json.status=='success') { 
                location.reload();
            }
        } 
    });
}

/* 댓글 등록 */
function setReplyWrite(userKey, socialType, socialKey, newsId, snsCont, newsContents, newsTitle, newsImage) {
    console.log("userKey==>"+userKey);
    console.log("socialType==>"+socialType);
    console.log("socialKey==>"+socialKey);
    console.log("newsId==>"+newsId);
    console.log("snsCont==>"+snsCont);
    console.log("newsContents==>"+newsContents);
    console.log("newsTitle==>"+newsTitle);
    console.log("newsImage==>"+newsImage);
    $.ajax({
        url: _SOCIAL_URI_API_,
        type: "POST",
        data: {
            'action':'setReplyWrite',
            'userKey':userKey,
            'socialType':socialType,
            'socialKey':socialKey,
            'newsId':newsId,
            'snsCont':snsCont,
            'newsContents':newsContents,
            'newsTitle':newsTitle,
            'newsImage':newsImage
        },
        success: function(data) {
            var json = jQuery.parseJSON(data);
            if(json.status==1) {
                $("#textarea_reply").val('');
                $("#textarea_len").find("strong").html('0');
                //setBtnEnable();
                setTotCnt('up');
                // getReplyAddOne(json.data);
                $("#sr_page").val('1');
                getReplyList(newsId);
            } else {
                //setBtnEnable();
                alert(json.msg);
            }
        } 
    });
}

/* 댓글리스트가져오기 */
function getReplyList(newsId) {
    var page = parseInt($("#sr_page").val());
    
    $.ajax({
        url: _SOCIAL_URI_UI_,
        data: {'action':'getReplyList','newsId':newsId,'page':page},
        success: function(data) {
            $("#sr_page").val(page);
            //console.log(data);
            $("#reply_list").html(data);
            eventInit();
        } 
    });
}

/* 댓글리스트가져오기 */
function getReplyListAdd(newsId){
    var page = parseInt($("#sr_page").val());
    page = page + 1;
    
    $.ajax({
        url: _SOCIAL_URI_UI_,
        data: {'action':'getReplyList','newsId':newsId,'page':page},
        success: function(data) {
            $("#sr_page").val(page);
            //console.log(data);
            $("#reply_list").append(data);
            eventInit();
        } 
    });
}

/* 토탈 카운트 업다운 */
function setTotCnt(type) {
    totCnt = $("#sr_tot_cnt").html();
    totCnt = parseInt(totCnt);
    if(type == 'up') {
        totCnt = totCnt + 1;
    } else { 
        totCnt = totCnt - 1;
    }
    $("#sr_tot_cnt").html(totCnt);
}

/* 댓글 삭제 */
function deleteReply(snsKey, newsId, authKey) {
    $.ajax({
        url: _SOCIAL_URI_API_,
        type: "POST",
        data: {
            'action':'deleteReply',
            'snsKey':snsKey,
            'authKey':authKey,
            'newsId':newsId
        },
        success: function(data) {
            var json = jQuery.parseJSON(data);
            if(json.status==1){
                setTotCnt('down');
                getReplyList(newsId);
                eventInit();
            } else {
                alert(json.msg);
            }
        } 
    });
}

/* 댓댓글 작성 */
function setCommentWrite(snsKey, newsId, socialKey, socialType, snsCmt){
    $.ajax({
        url: _SOCIAL_URI_API_,
        type: "POST",
        data: {
            'action':'setCommentWrite',
            'snsKey':snsKey,
            'newsId':newsId,
            'socialKey':socialKey,
            'socialType':socialType,
            'snsCmt':snsCmt
        },
        success: function(data) {
            var json = jQuery.parseJSON(data);
            if(json.status==1){
                setTaLenZero();
                getReply(snsKey,'normal');
            } else {
                alert(json.msg);
            }
        } 
    });
}

/* 텍스트 Area length 초기화 */
function setTaLenZero(){
    $("#txtarea_len").html('0');
}

/* 댓글 가져오기 */
function getReply(snsKey, type) {
    $.ajax({
        url: _SOCIAL_URI_UI_,
        data: {'action':'getReply','snsKey':snsKey,'type':type},
        success: function(data) {
            $("#reply_"+snsKey).html(data);
            eventInit();
        } 
    });
}

/* 댓글 가져오기 - type 2 : 댓글 입력시 */
function getReplyAddOne(snsKey) {
    $.ajax({
        url: _SOCIAL_URI_UI_,
        data: {'action':'getReply','snsKey':snsKey},
        success: function(data) {
            $("#reply_"+snsKey).before(data);
            eventInit();
        } 
    });
}

/* 댓글 코멘트 삭제*/
function deleteComment(cmtKey, snsKey, authKey) {
    $.ajax({
        url: _SOCIAL_URI_API_,
        type: "POST",
        data: {
            'action':'deleteComment',
            'cmtKey':cmtKey,
            'authKey':authKey,
            'snsKey':snsKey
        },
        success: function(data) {
            var json = jQuery.parseJSON(data);
            if(json.status==1){
                getReply(snsKey, 'normal');
                eventInit();
            }else{
                alert(json.msg);
            }
        } 
    });
}

/* 댓글 추천,비추천  */
function setReplyAction(snsKey, action, newsId) {
    $.ajax({
        url: _SOCIAL_URI_API_,
        type: "POST",
        data: {
            'action':'setReplyAction',
            'snsKey':snsKey,
            'act':action
        },
        success: function(data) {
            var json = jQuery.parseJSON(data);
            if(json.status==1) {
                action_num = parseInt($("#reply_action_" + action + "_" + snsKey).html());
                $("#reply_action_" + action + "_" + snsKey).html(action_num + 1);
                if(action == 'R') { 
                    alert('신고가 접수되었습니다');
                }
            } else {
                alert(json.msg);
            }
        } 
    });
} 

/* 댓글 코멘트 추천,비추천  */
function setCmtAction(snsKey, cmtKey, action) {
